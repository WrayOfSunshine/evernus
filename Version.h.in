#define MAJOR "${MAJOR_VERSION}"
#define MINOR "${MINOR_VERSION}"

namespace version {
constexpr const char *majorStr() { return MAJOR; }
constexpr const char *minorStr() { return MINOR; }
constexpr const char *fullStr() { return MAJOR "." MINOR; }

constexpr const char *dbTableName() { return "version"; }

constexpr unsigned int major() { return ${MAJOR_VERSION}; }
constexpr unsigned int minor() { return ${MINOR_VERSION}; }
}

#undef MINOR
#undef MAJOR
